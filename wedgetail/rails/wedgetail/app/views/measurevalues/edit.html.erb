<h1>Editing measurevalue</h1>

<% form_for(@measurevalue) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :patient %><br />
    <%= f.text_field :patient %>
  </p>
  <p>
    <%= f.label :measure_id %><br />
    <%= f.text_field :measure_id %>
  </p>
  <p>
    <%= f.label :value %><br />
    <%= f.text_field :value %>
  </p>
  <p>
    <%= f.label :value_date %><br />
    <%= f.date_select :value_date %>
  </p>
  <p>
    <%= f.label :created_by %><br />
    <%= f.text_field :created_by %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @measurevalue %> |
<%= link_to 'Back', measurevalues_path %>